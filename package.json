{
  "name": "excel-ai-formula-optimizer",
  "version": "1.0.0",
  "description": "AI-powered Excel formula optimization add-in",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "start": "office-addin-debugging start manifest.xml",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build"
  },
  "keywords": [
    "excel",
    "office-add-in",
    "ai",
    "formula-optimization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fluentui/react": "^8.119.0",
    "@fluentui/react-components": "^9.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/office-js": "^1.0.400",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.0",
    "html-webpack-plugin": "^5.6.0",
    "office-addin-debugging": "^5.1.0",
    "office-addin-dev-certs": "^1.13.5",
    "office-addin-manifest": "^1.13.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
